extends layout

block content
  h1= title
  - let image_names = ['cartoon.jpg','cartoon1.jpg','cartoon2.jpg','cartoon3.jpg','cartoon4.jpg']
  - for(let i=0;i<3;i++){
      - let rno = Math.floor(Math.random()*5)
      - let iname = image_names[rno]

      img(src=`images/${iname}` alt='cartoon' style='width:300px; height:300px;')
      |    
  - }